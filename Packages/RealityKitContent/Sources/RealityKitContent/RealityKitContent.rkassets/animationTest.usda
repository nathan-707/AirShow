        {
            over "RightOrthoPreviewSurface"
            {
                token inputs:frame:stPrimvarName = "st"
                token outputs:mtlx:surface
                token outputs:realitykit:vertex

                def Shader "diffuseColor"
                {
                    uniform token info:id = "UsdUVTexture"
                    asset inputs:file = @apache/TEX_LHC.jpg@
                    float2 inputs:st.connect = </Root/animationTest/Materials/RightOrthoPreviewSurface/diffuseColor/TexCoordReader.outputs:result>
                    color3f outputs:rgb

                    def Shader "TexCoordReader"
                    {
                        uniform token info:id = "UsdPrimvarReader_float2"
                        token inputs:varname.connect = </Root/animationTest/Materials/RightOrthoPreviewSurface/RightOrthoPreviewSurfacePreviewSurface.inputs:frame:stPrimvarName>
                        float2 outputs:result
                    }
                }

                over "RightOrthoPreviewSurfacePreviewSurface"
                {
