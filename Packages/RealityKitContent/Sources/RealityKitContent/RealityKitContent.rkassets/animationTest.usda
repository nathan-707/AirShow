#usda 1.0
(
    defaultPrim = "Root"
    metersPerUnit = 1
    upAxis = "Y"
)

def Xform "Root"
{
    def "animationTest" (
        active = true
        references = @apacheAnimated.usdz@
    )
    {
        float3 xformOp:scale = (0.959, 0.959, 0.959)
        uniform token[] xformOpOrder = ["xformOp:translate", "xformOp:orient", "xformOp:scale"]

