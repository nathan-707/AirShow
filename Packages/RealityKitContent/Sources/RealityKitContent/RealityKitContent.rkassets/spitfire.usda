                    }
                }

                over "surfaceShader"
                {
                    color3f inputs:diffuseColor.connect = </Root/spitfire/Materials/Pilot/diffuseColor.outputs:rgb>
                }
            }

            over "reflect"
            {
                token outputs:mtlx:surface
                token outputs:realitykit:vertex
            }
        }
    }
}

