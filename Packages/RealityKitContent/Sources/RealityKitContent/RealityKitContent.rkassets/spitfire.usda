                    }
                }

                over "surfaceShader"
                {
                    color3f inputs:diffuseColor.connect = </Root/spitfire/Materials/Body_Front/diffuseColor.outputs:rgb>
                }
            }

            over "Pilot"
            {
                token inputs:frame:stPrimvarName = "st"
                token outputs:mtlx:surface
                token outputs:realitykit:vertex

                def Shader "diffuseColor"
                {
                    uniform token info:id = "UsdUVTexture"
                    asset inputs:file = @Spitfire/tex/pilot_uk.png@
                    float2 inputs:st.connect = </Root/spitfire/Materials/Pilot/diffuseColor/TexCoordReader.outputs:result>
                    color3f outputs:rgb

                    def Shader "TexCoordReader"
                    {
                        uniform token info:id = "UsdPrimvarReader_float2"
